		<script>
		    $.each( $('.nav li a, .main-heading h1'), function () {
		      var niceText = $(this), openSpan = '<span>', closeSpan = '</span>';
		      /* Make the sentence into an array */
		      niceText = niceText.text().split(' ');
		      niceText.splice( 1, 0, openSpan );
		
		      /* Add span to the beginning of the array */
		      niceText.unshift( closeSpan );
		
		      /* Add  as the 4th value in the array */
		
		      /* Turn it back into a string */
		      niceText = niceText.join(' ');
		      $(this).html(niceText);
		    });
		    $( ".nav li a" ).css( "fontSize", "20px" );
		    
		    $(document).ready(function(){
			       $('.main-menu-wrap > .container').fadeIn(); 
		    });
		</script>
